{
  "name": "super-duper-eureka",
  "type": "module",
  "private": true,
  "workspaces": ["packages/*", "apps/*"],
  "scripts": {
    "retirement-calc": "bun run packages/cli/src/index.ts",
    "test": "bun run --parallel --filter '!e2e' test",
    "lint": "bun run --parallel --filter '*' lint",
    "format": "bun run --parallel --filter '*' format",
    "build": "bun run --parallel --filter '*' build",
    "dev": "bun run --parallel --filter '*' dev",
    "e2e": "bun run --filter e2e test",
    "e2e:debugger": "bun run --filter e2e test:debugger",
    "e2e:ui": "bun run --filter e2e test:ui"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
